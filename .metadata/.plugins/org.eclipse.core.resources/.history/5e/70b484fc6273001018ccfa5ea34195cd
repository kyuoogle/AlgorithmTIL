package secondDimensionArray;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

public class SWEA_4831 {
public static void main(String[] args) throws IOException{
		
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

		int T = Integer.parseInt(br.readLine());
		
		for(int tc = 1; tc <= T; tc++) {
			StringTokenizer st = new StringTokenizer(br.readLine());
			
			int K = Integer.parseInt(st.nextToken()); // 최대 이동 가능 거리
			int N = Integer.parseInt(st.nextToken()); // 종점
			int M = Integer.parseInt(st.nextToken()); // 충전기가 설치된 정류장
			
			int[] station = new int[N + 1];
			
			st = new StringTokenizer(br.readLine());
			
		for(int i = 0; i < M; i++) {
			int charger = Integer.parseInt(st.nextToken());
			station[charger] = 1;
		}
		int now = 0;
		int count = 0;
		boolean success = true;
		
		while(now + K < M) {
			boolean found = false;
			
			for(int i = K; i >= M; i--) {
				if(station[now + 1] == 1) {
					now += i;
					count++;
					found = true;
					break;
				}
			}
			if(!found ) {
				success = false;
				break;
			}
		}
		System.out.println("#" + tc + " " + (success ? count : 0));
		}
	}	
}