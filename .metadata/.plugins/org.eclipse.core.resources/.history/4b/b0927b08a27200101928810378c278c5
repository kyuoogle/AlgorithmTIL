import java.util.Scanner;

class SWEA_4835 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int T = sc.nextInt();
        for (int t = 1; t <= T; t++) {
            int N = sc.nextInt();
            int M = sc.nextInt();
            int[] nums = new int[N];
            for (int i = 0; i < N; i++) {
                nums[i] = sc.nextInt();
            }

            int max_sum = -1;
            int min_sum = Integer.MAX_VALUE;

            for (int i = 0; i <= N - M; i++) {
                int current_sum = 0;
                for (int j = i; j < i + M; j++) {
                    current_sum += nums[j];
                }
                max_sum = Math.max(max_sum, current_sum);
                min_sum = Math.min(min_sum, current_sum);
            }
            System.out.println("#" + t + " " + (max_sum - min_sum));
        }
        sc.close();
    }
}
