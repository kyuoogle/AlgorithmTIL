package secondDimensionArray;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class SWEA_4834 {
    public static void main(String[] args) throws IOException {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        StringBuilder sb = new StringBuilder();

        int T = Integer.parseInt(br.readLine()); // 테스트 케이스 수

        for (int tc = 1; tc <= T; tc++) {
            int N = Integer.parseInt(br.readLine()); // 숫자 카드 수 (실제로는 사용하지 않음)
            String numString = br.readLine();        // 숫자 카드 문자열 입력

            int[] counting = new int[10]; // 0~9 숫자의 개수를 셀 배열

            // 문자열을 한 글자씩 순회하여 숫자 개수 세기
            for (int i = 0; i < N; i++) {
                char c = numString.charAt(i);
                counting[c - '0']++;
            }

            // 가장 많이 나온 숫자(동점이면 큰 수 우선) 찾기
            int ans = 0;
            for (int i = 1; i < 10; i++) {
                if (counting[i] >= counting[ans]) {
                    ans = i;
                }
            }

            // 결과 저장
            sb.append("#").append(tc).append(" ")
              .append(ans).append(" ")
              .append(counting[ans]).append('\n');
        }

        // 전체 결과 출력
        System.out.print(sb);
    }
}
